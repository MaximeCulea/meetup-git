{
	"name"       : "rarst/laps",
	"description": "Light WordPress profiler.",
	"keywords"   : ["wordpress", "performance"],
	"type"       : "wordpress-plugin",
	"homepage"   : "https://github.com/Rarst/laps",
	"license"    : "MIT",
	"authors"    : [
		{
			"name"    : "Andrey Savchenko",
			"homepage": "http://www.Rarst.net/"
		}
	],
	"support"    : {
